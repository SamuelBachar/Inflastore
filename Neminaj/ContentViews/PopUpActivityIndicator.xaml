<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Neminaj.ContentViews.PopUpActivityIndicator">

    <VerticalStackLayout VerticalOptions="Center" HorizontalOptions="Center">
        <ActivityIndicator x:Name="ActivityIndicator" IsRunning="False" Scale="2" HorizontalOptions="Center" VerticalOptions="Center" Margin="0,0,0,40" />
        <Label x:Name="lblPopUp" Text="" VerticalOptions="Center" HorizontalOptions="Center" />
    </VerticalStackLayout>
</ContentView>
